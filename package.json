{
  "name": "swissdlt-faucet-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Pascal Marco Caversaccio",
  "description": "An Angular-based frontend for the Ether faucet of the Swiss DLT blockchain.",
  "keywords": [
    "angular",
    "ethereum",
    "blockchain",
    "faucet",
    "web3",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.appswithlove.net/swissdlt/swissdlt-faucet-frontend.git"
  },
  "bugs": {
    "url": "https://gitlab.appswithlove.net/swissdlt/swissdlt-faucet-frontend/-/issues"
  },
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve -o",
    "build": "ng build",
    "build:prod": "ng build --prod --aot",
    "build:ci": "npm run build:prod --base-href=/swissdlt-faucet-frontend/",
    "lint": "ng lint",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepush": "npm run lint",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.2",
    "@angular/cdk": "^12.0.2",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/material": "^12.0.2",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/router": "~12.0.2",
    "assert": "^2.0.0",
    "crypto-browserify": "^3.12.0",
    "http-server": "^0.12.3",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "~6.6.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tslib": "^2.1.0",
    "web3": "^1.3.6",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.2",
    "@angular-eslint/builder": "12.0.0",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/schematics": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "~12.0.2",
    "@angular/compiler-cli": "~12.0.2",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "^7.26.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}